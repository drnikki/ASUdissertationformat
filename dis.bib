@article{shanahan2014,
  title = {Enforcing the {{Colorline}} and {{Counting White Races}}: {{Race}} and the {{Census}} in {{North America}}, 1900\textendash{}1941.},
  volume = {44},
  issn = {02722011},
  abstract = {Between 1900 and 1941, American and Canadian census planners devised queries about race in disparate ways. Whereas Americans were identified as members of white, black, Amerindian, or various Asian ``races,'' only once were Canadians similarly classified. Canadian authorities instead recorded the historic ``racial origins'' of the population (e.g. English, French, or Chinese). These different approaches reflected the primary ways in which race was understood in both the US (the colorline) and Canada (Anglo- and French Canadian biculturalism). American and Canadian planners, however, sought demographic information that did not align with their respective nation's framework. Therefore, they tweaked their queries. In the US the mother tongue and national origin of immigrants became proxies for race while Canadians structured their ``racial origins'' queries within a white/nonwhite binary. While such efforts led to many similar forms of racial categorization, differing primary frameworks towards race on the American and Canadian censuses prevented total accord.},
  language = {eng},
  number = {3},
  journal = {American Review of Canadian Studies},
  author = {Shanahan, Brendan A.},
  month = sep,
  year = {2014},
  keywords = {opendemographics},
