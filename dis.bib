  pages = {20160112},
  file = {/Users/nikki/Dropbox/ASU/zotero/Floridi - 2016 - Faultless responsibility on the nature and alloca.pdf;/Users/nikki/Zotero/storage/HET6GW7W/20160112.html},
  pmid = {28336791}
}

@article{thow-yick1994,
  title = {The Basic Entity Model: A Fundamental Theoretical Model of Information and Information Processing},
