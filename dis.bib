  file = {/Users/nikki/Zotero/storage/MTKIXE39/Bowman and Madsen - 2018 - Queer identities in materials science and engineer.pdf;/Users/nikki/Zotero/storage/HHGIVTY9/2D31E21AB53C7133A4C3632069E8448E.html}
}

