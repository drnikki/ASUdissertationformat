  journal = {BMC Public Health},
  doi = {10.1186/s12889-016-3560-5},
  author = {Zeluf, Galit and Dhejne, Cecilia and Orre, Carolina and Nilunger Mannheimer, Louise and Deogan, Charlotte and H{\"o}ijer, Jonas and Ek{\'e}us Thorson, Anna},
  month = dec,
